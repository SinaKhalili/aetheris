{
    "name": "@aetheris/server",
    "version": "0.0.4",
    "license": "MIT",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": "./dist/index.js",
        "./adapters/http": "./dist/adapters/http.js",
        "./adapters/next": "./dist/adapters/next.js",
        "./core": "./dist/core/index.js"
    },
    "scripts": {
        "dev": "tsup --format cjs,esm --dts --watch",
        "build": "tsup --format cjs,esm --dts"
    },
    "devDependencies": {
        "@types/node": "^20.14.2",
        "next": "^14.2.4",
        "typescript": "^5.4.5",
        "zod": "^3.23.8"
    },
    "tsup": {
        "entryPoints": {
            "index": "src/index.ts",
            "adapters/http": "src/adapters/http.ts",
            "adapters/next": "src/adapters/next.ts"
        }
    },
    "dependencies": {
        "ws": "^8.17.1"
    }
}
